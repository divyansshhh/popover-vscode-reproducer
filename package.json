{
  "name": "modal-vscode-reproducer",
  "displayName": "modal-vscode-reproducer",
  "description": "reproducer for opening modals in vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "keywords": [
    "notebookRenderer"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension/extension.js",
  "browser": "./out/extension/extension.web.js",
  "contributes": {
    "notebookRenderer": [
      {
        "id": "modal-vscode-reproducer",
        "entrypoint": "./out/client/index.js",
        "displayName": "modal-vscode-reproducer",
        "mimeTypes": ["x-application/custom-json-output"]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "webpack --mode development --watch",
    "pretest": "webpack --mode development && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@types/vscode-notebook-renderer": "^1.72.0",
    "@types/vscode": "^1.82.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.47.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "vscode-notebook-error-overlay": "^1.0.1",
    "@vscode/test-electron": "^2.3.4",
    "util": "^0.12.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
